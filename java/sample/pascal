/*package whatever //do not write package name here */

import java.io.*;

class GFG {
	public static void main (String[] args) {
	   int n=5;
	  
	   for(int i=1;i<=n;i++)
	   {
	        int k=1;
	        for(int s=n-1;s>=i;s--)
	           {
	               System.out.print(" ");
	           }
	       for(int j=1;j<=i;j++)
	       {   
	           System.out.print(k+" "); 
               k = k * (i - j) / j; 
	           
	       }
	     
	       System.out.println();
	   }
	}
}
